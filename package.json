{
  "name": "canrau.com",
  "version": "0.1.0",
  "description": "Website & Blog of Can Rau",
  "keywords": [],
  "homepage": "https://www.canrau.com",
  "bugs": {
    "url": "https://github.com/canrau/canrau.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canrau/canrau.com.git"
  },
  "license": "MIT",
  "author": "Can Rau <cansrau@gmail.com> (https://www.canrau.com)",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "//dev": "vite",
    "//build": "vite build",
    "dev": "pnpm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && vite build --ssr",
    "server": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "build:analyze": "vite build --mode=analyze",
    "preview": "vite preview",
    "windicss": "windicss-analysis"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "pnpx sort-package-json"
  },
  "dependencies": {
    "@vitejs/plugin-legacy": "1.4.4",
    "chalk": "4.1.1",
    "daisyui": "1.10.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "mdx-bundler": "5.1.1",
    "postcss-rename": "0.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup-plugin-postcss": "4.0.0",
    "vite": "2.4.2",
    "vite-plugin-mkcert": "1.3.2",
    "vite-plugin-restart": "0.0.2",
    "vite-plugin-ssr": "0.1.5",
    "windicss": "3.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/express": "4.17.13",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/rollup-plugin-visualizer": "4.2.1",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "@vitejs/plugin-react-refresh": "1.3.5",
    "eslint": "7.30.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "rollup-plugin-visualizer": "5.5.2",
    "sort-package-json": "1.50.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "vite-plugin-windicss": "1.2.4",
    "vite-react-jsx": "1.1.2",
    "vite-tsconfig-paths": "3.3.13",
    "windicss-analysis": "0.3.4"
  }
}
