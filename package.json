    "@react-aria/tooltip": "3.1.3",
    "@react-aria/utils": "3.8.2",
    "@react-aria/visually-hidden": "3.2.3",
    "@spectrum-icons/illustrations": "3.2.2",
    "@spectrum-icons/workflow": "3.2.1",
    "@storybook/react": "^6.4.0-alpha.18",
    "@vitejs/plugin-legacy": "1.5.1",
    "aws-lambda": "1.0.6",
    "chalk": "4.1.1",
    "chokidar": "3.5.2",
    "clsx": "1.1.1",
    "downshift": "6.1.3",
    "esbuild": "0.12.17",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "mdx-bundler": "5.1.1",
    "path-to-regexp": "6.2.0",
    "postcss-rename": "0.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-obfuscate": "3.6.8",
    "rollup-plugin-postcss": "4.0.0",
    "tailwind-nord": "1.2.0",
    "vite": "2.5.0",
    "vite-plugin-mkcert": "1.5.1",
    "vite-plugin-restart": "0.0.2",
    "vite-plugin-ssr": "0.2.9",
    "windicss": "3.1.5"
  },
  "devDependencies": {
